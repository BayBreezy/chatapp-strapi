{
  "compilerOptions": {
    "types": [
      "@nuxt/types",
      "@nuxtjs/vuetify",
      "@nuxtjs/dayjs",
      "cookie-universal-nuxt"
    ],
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "~~/*": ["./*"],
      "@@/*": ["./*"]
    }
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
